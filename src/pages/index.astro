---
const data = await fetch('https://west.albion-online-data.com/api/v2/stats/prices/T4_BAG,T5_BAG?locations=Caerleon,Bridgewatch&qualities=2').then((response)=>response.json());
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
			{data.map((dat)=>(
				<p>{dat.item_id}</p>
				<p>{dat.city}</p>
				<p>{dat.quality}</p>
				<p>{dat.sell_price_min}</p>
			))}
	</body>
</html>
